<UserControl x:Class="Beutl.CustomTabSample.SampleToolTabView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="using:Beutl.CustomTabSample"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="450"
             d:DesignWidth="800"
             x:CompileBindings="True"
             x:DataType="local:SampleToolTabViewModel"
             mc:Ignorable="d">
    <Arc x:Name="arc"
         Width="100"
         Height="100"
         HorizontalAlignment="Center"
         VerticalAlignment="Center"
         StartAngle="{Binding StartAngle.Value}"
         Stroke="{DynamicResource SystemAccentColor}"
         StrokeThickness="10"
         SweepAngle="{Binding SweepAngle.Value}" />
</UserControl>
